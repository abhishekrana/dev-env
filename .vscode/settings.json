{
    "AREPL.telemetry": false,
    "autoDocstring.docstringFormat": "google",
    "code-runner.enableAppInsights": false,
    "debug.toolBarLocation": "docked",
    "docker-explorer.enableTelemetry": false,
    "editor.copyWithSyntaxHighlighting": true,
    "editor.cursorSmoothCaretAnimation": false,
    "editor.fontLigatures": true,
    "editor.largeFileOptimizations": false,
    "editor.minimap.enabled": false,
    "editor.occurrencesHighlight": false,
    "editor.renderIndentGuides": false,
    "editor.rulers": [
        120
    ],
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "files.associations": {
        "Dockerfile-*": "dockerfile",
        "Dockerfile.*": "dockerfile",
        "Dockerfile_*": "dockerfile"
    },
    "files.exclude": {
        "**/.git": true,
        "**/__pycache__/": true
    },
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "githubPullRequests.telemetry.enabled": false,
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.statusBar.enabled": false,
    "outline.showProperties": false,
    "outline.showVariables": false,
    "python.dataScience.sendSelectionToInteractiveWindow": false,
    "python.envFile": "${workspaceFolder}/.env",
    "python.formatting.blackArgs": [
        "--line-length",
        "120"
    ],
    "python.formatting.provider": "black",
    // "python.languageServer": "Microsoft",
    // "python.linting.flake8Args": [
    //     "--max-line-length=120",
    //     "--ignore=F841"
    // ],
    // "python.linting.flake8Enabled": true,
    // "python.linting.mypyArgs": [
    //     "--ignore-missing-imports",
    //     "--follow-imports=silent",
    //     "--check-untyped-defs",
    //     "--disallow-subclassing-any",
    //     "--disallow-untyped-decorators",
    //     "--no-implicit-optional",
    //     "--strict-optional",
    //     "--warn-redundant-casts",
    //     "--warn-return-any",
    //     "--warn-unused-ignores",
    //     "--warn-unused-configs"
    // ],
    // "python.linting.mypyEnabled": true,
    // "python.linting.pycodestyleEnabled": false,
    // "python.linting.pydocstyleArgs": [
    //     "--convention=google",
    //     "--ignore=D100,D101,D102,D103,D104,D105,D106,D107,D213"
    // ],
    // "python.linting.pydocstyleEnabled": true,
    // "python.linting.pylintEnabled": false,
    // "python.pythonPath": "/usr/bin/python3.6",
    //"python.analysis.diagnosticMode": "workspace",
    "python.analysis.extraPaths": ["project_name/module_name/src"],
    "python.analysis.typeCheckingMode": "strict",
    "python.languageServer": "Pylance",
    "python.linting.pylintEnabled": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${env:PYTHONPATH}:${workspaceFolder}/project_name/module_name/src"
    },
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.scrollback": 10000,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.zoomLevel": 0,
    "workbench.colorCustomizations": {},
    "workbench.colorTheme": "Solarized Light",
    "workbench.editor.enablePreview": false,
    "workbench.sideBar.location": "right",
    "workbench.tips.enabled": true,
}